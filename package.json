{
	"name": "@barelyhuman/tocolor",
	"version": "0.0.9",
	"keywords": [
		"color",
		"conversion",
		"rgb to hex",
		"hex to rgb",
		"hsl to rgb",
		"tiny"
	],
	"files": [
		"esm",
		"dist",
		"src"
	],
	"repository": "git@github.com:barelyhuman/tocolor.git",
	"license": "MIT",
	"author": "Reaper <ahoy@barelyhuman.dev> (https://reaper.im)",
	"exports": {
		".": {
			"import": "./index.mjs",
			"require": "./index.js",
			"types": "./index.d.ts"
		},
		"./package.json": "./package.json"
	},
	"main": "./dist/index.js",
	"module": "./esm/index.js",
	"types": "./index.d.ts",
	"scripts": {
		"build": "npm run build:cjs;npm run build:esm",
		"build:cjs": "tsc --module commonjs --outDir dist",
		"build:esm": "tsc --module esnext --outDir esm",
		"fix": "prettier --write .",
		"size": "sizesnap",
		"test": "uvu -r tsm tests"
	},
	"prettier": "@barelyhuman/prettier-config",
	"devDependencies": {
		"@barelyhuman/prettier-config": "^0.1.0",
		"prettier": "^2.5.1",
		"sizesnap": "^0.1.0",
		"tsm": "^2.2.1",
		"typescript": "^4.5.5",
		"uvu": "^0.5.1"
	},
	"publishConfig": {
		"access": "public"
	},
	"sizesnap": {
		"files": [
			"./dist/**/*",
			"./esm/**/*"
		]
	}
}
